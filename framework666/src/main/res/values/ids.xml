<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--如果布局拥有Appbar，那么AppbarLayout的复层一定要使用此id-->
    <item name="appbarLayoutParent" type="id"/>

    <!--布局的Appbar-->
    <item name="appbarLayout" type="id" />

    <!--如果布局拥有Appbar，且内部使用滑动变动高度效果，那么CollapsingToolbarLayout一定要使用此id-->
    <item name="collapsingToolbarLayout" type="id"/>

    <!--Appbar中的toolbar中的id-->
    <item name="toolbar" type="id"/>

    <!--在Toolbar中负责显示title的TextView的id-->
    <item name="toolbarTitle" type="id"/>

    <!--ComponentX容器View的id-->
    <item name="componentXContainer" type="id" />
</resources>